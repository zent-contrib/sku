{
  "name": "@zent/sku",
  "version": "1.0.0",
  "scope": "zent",
  "description": "sku React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --no-demo && ./scripts/replace-es-with-cjs.sh && npm run build-css --scripts-prepend-node-path",
